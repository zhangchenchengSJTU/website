@misc{iyamaAuslanderReitenTheoryExtriangulated2018,
  title = {Auslander--{{Reiten}} Theory in Extriangulated Categories},
  author = {Iyama, Osamu and Nakaoka, Hiroyuki and Palu, Yann},
  year = {2018},
  publisher = {arXiv},
  doi = {10.48550/ARXIV.1805.03776},
  urldate = {2025-09-23},
  abstract = {The notion of an extriangulated category gives a unification of existing theories in exact or abelian categories and in triangulated categories. In this article, we develop Auslander--Reiten theory for extriangulated categories. This unifies Auslander--Reiten theories developed in exact categories and triangulated categories independently. We give two different sets of sufficient conditions on the extriangulated category so that existence of almost split extensions becomes equivalent to that of an Auslander--Reiten--Serre duality. We also show that existence of almost split extensions is preserved under taking relative extriangulated categories, ideal quotients, and extension-closed subcategories. Moreover, we prove that the stable category \${\textbackslash}underline\{{\textbackslash}mathscr\{C\}\}\$ of an extriangulated category \${\textbackslash}mathscr\{C\}\$ is a \${$\tau\$$}-category if \${\textbackslash}mathscr\{C\}\$ has enough projectives, almost split extensions and source morphisms. This gives various consequences on \${\textbackslash}underline\{{\textbackslash}mathscr\{C\}\}\$, including Igusa--Todorov's Radical Layers Theorem, Auslander--Reiten Combinatorics on dimensions of Hom-spaces, and Reconstruction Theorem of the associated completely graded category of \${\textbackslash}underline\{{\textbackslash}mathscr\{C\}\}\$ via the complete mesh category of the Auslander--Reiten species of \${\textbackslash}underline\{{\textbackslash}mathscr\{C\}\}\$. Finally we prove that any locally finite symmetrizable \${$\tau\$$}-quiver (=valued translation quiver) is an Auslander--Reiten quiver of some extriangulated category with sink morphisms and source morphisms.},
  copyright = {arXiv.org perpetual, non-exclusive license},
  keywords = {16G70 18E10 18E30 16E30 16G50,Category Theory (math.CT),FOS: Mathematics,Representation Theory (math.RT)}
}

